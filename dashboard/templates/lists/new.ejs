<%- include('../blocks/header', { bot, user, title: "Recent Added Bots | Discord Bot House", embedding: { default: true } }) %>

<meta name="description" content="Discover newly added bots!">
<meta name="og:title" property="og:title" content="New bots | Discord Bot House">
<meta name="og:description" property="og:description" content="Discover newly added bots!">
<meta name="twitter:card" content="Discover newly added bots!">

<div class="container1 main-bkg" style="margin-left: 3%; margin-right: 3%;margin-top: 5px;margin-bottom: 20px;box-shadow:0 8px 16px 0 rgba(0, 0, 0, 1.2); color: #fff; background-color: #2b828e;">
<h2 style="padding-top: 20px; margin-left: 3%;color:white"><i class="fas fa-hourglass"></i> New Added Bots:</h2>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

<div style="padding: 35px 20px 20px 35px">
<div class="row">
  <% featuredBots.forEach(theBot => { if (!bot.users.get(theBot.id)) bot.users.get(theBot.id) = { presence: { status: "offline" }, username: theBot.name  } %>
    <%- include("../blocks/card.ejs", { theBot, bot }); %>
  <% }); %>
</div>
<center><nav aria-label="...">
  <ul class="pagination pagination-lg">
    <% for (var i = 0; i < numberOfPages; i++) { %>
      <li class="page-item"><a class="page-link" href="/list/new?page=<%= i + 1 %>"><%= i + 1 %></a></li>
     <% } %>
  </ul>
</nav></center>
</div>
</div>

